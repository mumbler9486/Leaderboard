{
	"name": "pso2-ngs-leaderboard",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": ">=22"
	},
	"scripts": {
		"dev:mock": "cross-env VITE_MSW_ENABLED=true vite dev",
		"build": "vite build && rimraf build/static/mockServiceWorker.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --plugin-search-dir . --write .",
		"start-local-db": "docker compose -f db_mocks/docker-compose.yml up -d",
		"reload-db": "node ./db_mocks/reloadDb.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^7.2.5",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/vite": "^4.1.17",
		"@testing-library/svelte": "^5.2.9",
		"cross-env": "^7.0.3",
		"daisyui": "^5.0.43",
		"msw": "^2.4.9",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"rimraf": "^5.0.5",
		"shelljs": "^0.8.5",
		"svelte": "^5.46.4",
		"svelte-check": "^4.0.0",
		"svelte-heros-v2": "2.0.1",
		"svelte-i18n": "^4.0.0",
		"svelte-local-storage-store": "^0.6.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^6.3.5",
		"vitest": "3.2.4",
		"wrangler": "^4.50.0",
		"yup": "^1.3.2"
	},
	"type": "module",
	"msw": {
		"workerDirectory": "static"
	}
}
