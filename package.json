{
	"name": "leaderboard",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev:mock": "cross-env VITE_MSW_ENABLED=true vite dev",
		"dev:swa": "swa start http://localhost:5173 --api-location api --run \"vite dev\"",
		"build": "vite build && rimraf build/static/mockServiceWorker.js",
		"preview": "swa start http://localhost:4173 --api-location api --run \"vite preview\" --open",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@azure/static-web-apps-cli": "^1.1.4",
		"@sveltejs/kit": "^1.23.0",
		"autoprefixer": "^10.4.15",
		"cross-env": "^7.0.3",
		"daisyui": "^3.9.3",
		"msw": "^1.2.4",
		"postcss": "^8.4.28",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.3",
		"rimraf": "^5.0.1",
		"svelte": "^4.2.0",
		"svelte-adapter-azure-swa": "^0.19.0",
		"svelte-check": "^3.5.0",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.1.6",
		"vite": "^4.4.9"
	},
	"type": "module",
	"dependencies": {
		"@azure/storage-blob": "^12.15.0",
		"dotenv": "^16.3.1",
		"mssql": "^9.1.3",
		"svelte-i18n": "^3.7.0",
		"svelte-local-storage-store": "^0.6.0",
		"yup": "^1.2.0"
	},
	"msw": {
		"workerDirectory": "static"
	}
}
